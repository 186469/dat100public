
public class Human22 {

	String name;
	int age;
	double weight;
	
	
	Human22(String name, int age, double weight) {
		
		this.name = name;
		this.age = age;
		this.weight = weight;
		
		 
	}
	
	void eat() {
		System.out.println(this.name +" is eating");
	}
	
	void drink() {
		System.out.println(this.name + " is drinking");
	}
	
	void shiting() {
		System.out.println(this.name +" is shitting");
	}
}
