import java.util.ArrayList;

public class main17arraylist {

	public static void main(String[] args) {
		
		// Array list = et skalerbart array.
		// Elementer kan legges til og fjernes etter kompilasjonsfasen lagrer referansedatatyper
		
		//Opretter array list som kan lagre "string-elementer"
		ArrayList<String> food = new ArrayList<String>();
		
		//Elementer i listen
		food.add("pizza");
		food.add("p√∏lse");
		food.add("hamburger");
		food.add("taco");
		
		
		food.set(3, "sushi");	//bytter pizza(0) med sushi
		
		
		for(int i = 0; i<food.size(); i++) {
			System.out.println(food.get(i));
			
		}
		

	}

}
