
public class main22Constructor {

	public static void main(String[] args) {
		
		// Constructor = Spesiell metode som kalles n√•r et objekt blir instansiert (opprettet)
		Human22 human = new Human22("Rick", 65, 70);
		Human22 human2 = new Human22("Morty", 16, 50);
		
		System.out.println(human2.age); 
		
	}

}
