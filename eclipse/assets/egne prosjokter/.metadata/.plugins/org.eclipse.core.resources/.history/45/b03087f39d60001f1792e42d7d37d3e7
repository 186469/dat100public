
public class main15array {

	public static void main(String[] args) {

		// array = Brukes til Ã¥ lagre flere verdier i en enkel variabel
		
		String[] cars = {"Camero", "Corvette", "Tesla"};
		
		cars[0] = "Mustang";
		cars[1] = "Camero";
		cars[2] = "Corvette";
		cars[3] = "Tesla";
		
		
		System.out.println(cars[1]);
		
	}

}
 