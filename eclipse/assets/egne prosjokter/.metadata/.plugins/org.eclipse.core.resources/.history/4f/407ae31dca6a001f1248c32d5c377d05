
public class mainOOP21 {

	public static void main(String[] args) {
		
		// object = en instans av en klasse som kan inneholde attributter og metoder
		// eks: (tlf, skrivebord, datamaskin, kaffekopp)
		
		bil minBil = new bil();	//Bruker "bil" til Ã¥ hente info fra bil classen
		bil minBil2 = new bil();
		
		//Henter info fra bil.java classen
		System.out.println(minBil.make);
		System.out.println(minBil.model);
		System.out.println();
		System.out.println(minBil2.make);
		System.out.println(minBil2.model);
		
