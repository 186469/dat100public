
import static javax.swing.JOptionPane.*;


public class test {

	public static void main(String[] args) {
		
		String brukernavn = showInputDialog("Brukernavn");
		String bbrukernavn = showInputDialog("Brukernavn");
		
		String passord = showInputDialog("Passord");
		String ppassord = showInputDialog("Passord");
		
		if (bbrukernavn.equals(brukernavn) && ppassord.equals(passord)) {
			showMessageDialog(null, "Du skrev inn brukernavn og passord");
		}
			else {
				System.out.println("Du skrev feil brukernavn eller passord");
			}
		System.out.println(brukernavn);
		System.out.println(passord);
		
		}
		
		}

	
		
	


