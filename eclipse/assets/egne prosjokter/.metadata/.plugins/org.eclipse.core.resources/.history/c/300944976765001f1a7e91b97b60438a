
public class main19overloadedmethod {

	public static void main(String[] args) {
		
	// Overloaded methods = Metoder som har samme navn, men forskjellige parametere
	// metodens navn + parametere = metodens signatur
		
		double x = add(1.0, 2.0, 3.0, 4.0);
		
		System.out.println(x);
	}
	static int add(int a, int b) {
		System.out.println("Dette er overloaded method #1");
		return a + b;
		
}
	static int add(int a, int b, int c) {
		System.out.println("Dette er overloaded method #2");
		return a + b + c;
		
}
	static int add(int a, int b, int c, int d) {
		System.out.println("Dette er overloaded method #3");
		return a + b + c + d;
		
	} 
	static double add(double a, double b) {
		System.out.println("Dette er overloaded method #4");
		return a + b;
		
}
	static double add(double a,double b, double c) {
		System.out.println("Dette er overloaded method #5");
		return a + b + c;
		
}
	static double add(double a, double b, double c, double d) {
		System.out.println("Dette er overloaded method #6");
		return a + b + c + d;
	} 
}